{
  "configs": [
  ],
  "traits": {
    "container.enabled": "true",
    "container.request-cpu": "250m",
    "container.request-memory": "256Mi",
    "container.limit-cpu": "500m",
    "container.limit-memory": "512Mi",
    "health.enabled": "true",
    "health.liveness-probe-enabled": "true",
    "health.liveness-scheme": "HTTP",
    "health.liveness-initial-delay": "0",
    "health.liveness-timeout": "10",
    "health.liveness-period": "15",
    "health.liveness-success-threshold": "1",
    "health.liveness-failure-threshold": "3",
    "health.readiness-probe-enabled": "true",
    "health.readiness-scheme": "HTTP",
    "health.readiness-initial-delay": "0",
    "health.readiness-timeout": "10",
    "health.readiness-period": "15",
    "health.readiness-success-threshold": "1",
    "health.readiness-failure-threshold": "3",
    "knative-service.enabled": "true",
    "knative-service.autoscaling-class": "kpa.autoscaling.knative.dev",
    "knative-service.autoscaling-metric": "rps",
    "knative-service.autoscaling-target": "100",
    "knative-service.min-scale": "0",
    "knative-service.max-scale": "20",
    "prometheus.enabled": "true",
    "prometheus.pod-monitor": "true",
    "prometheus.pod-monitor-labels": "release=kube-prometheus-stack",
    "telemetry.enabled": "true",
    "telemetry.sampler": "on",
    "telemetry.auto": "true",
    "telemetry.endpoint": "http://instance-collector.observability.svc.cluster.local:4317",
    "telemetry.service-name": "mambu-customer-connector"
  }
}
